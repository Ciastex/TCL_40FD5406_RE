<?xml version="1.0" encoding="UTF-8"?>
<manifest package="SystemUI" xmlns:tvos="tvos" >

    <uses-sdk tvos:targetSdkVersion="1" ></uses-sdk>
    <set-env>
        <env tvos:name="LD_LIBRARY_PATH" tvos:mode="front">/system/tbrowser2/lib</env>
    </set-env>
    <application tvos:persistet="true">
        <activity
            tvos:name="SystemUI" >
            <intent-filter>
                <action tvos:name="tvos.intent.action.MAIN" />
                <category tvos:name="tvos.intent.category.HOME" />
            </intent-filter>
            <intent-filter>
                <action tvos:name="tvos.SystemUI.SystemUI" />
                <category tvos:name="tvos.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>